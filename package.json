{
  "name": "xaml-lang",
  "displayName": "XAML Language",
  "description": "Syntax highlighting for .NET XAML files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [{
      "path": "./syntaxes/xamlInjection.json",
      "scopeName": "source.xaml",
      "injectTo": ["source.xml"], 
      "tokenTypes": {
        "string.quoted.double.xml": "other"
      }
    }]
  }
}
